  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define", "BUILD_FLAVOR=debug"]
    },
    {
      "name": "Flutter Build Runner",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "request": "launch",
      "type": "dart",
      "args": ["build", "--delete-conflicting-outputs"]
    }
  ]
}
